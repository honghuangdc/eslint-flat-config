{
  "cSpell.words": [
    "antfu",
    "camelcase",
    "dprint",
    "espree",
    "linebreak",
    "lockb",
    "meriyah",
    "nuxt",
    "packagejson",
    "vitepress",
    "whitespaces"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never"
  },
  "editor.formatOnSave": false,
  "eslint.experimental.useFlatConfig": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue",
    "svelte",
    "astro",
    "html",
    "css",
    "less",
    "scss",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "yml",
    "markdown",
    "toml"
  ],
  "prettier.enable": false
}

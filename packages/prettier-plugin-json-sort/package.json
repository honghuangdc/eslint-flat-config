{
  "name": "prettier-plugin-json-sort",
  "type": "module",
  "version": "1.0.8",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm typecheck && pnpm build-only",
    "build-only": "unbuild",
    "stub": "unbuild --stub",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "peerDependencies": {
    "prettier": ">=2.0.0"
  },
  "devDependencies": {
    "typescript": "5.3.3",
    "unbuild": "2.0.0"
  }
}
